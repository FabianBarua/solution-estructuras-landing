{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": [".src/*"],
      "@components/*": ["src/components/*"],
      "@react/*": ["src/components/react/*"],
      "@pages/*": ["src/pages/*"],
      "@icons/*": ["src/components/icons/*"],
      "@layouts/*": ["src/layouts/*"],
      "@db/*": ["db/*"],
    }
  }
}